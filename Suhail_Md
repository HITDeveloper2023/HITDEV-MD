FROM quay.io/hitdeveloper/hitdev-v1
RUN git clone https://github.com/HITDeveloper2023/HITDEV-MD /root/Hidtev
WORKDIR /root/Hitdev
RUN npm install
EXPOSE 8000
CMD ["npm","start" ] 
































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































